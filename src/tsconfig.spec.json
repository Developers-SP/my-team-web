{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "*": [
        "*",
        "src/*",
        "app/*",
        "environments/*"
      ],
      "src/*": [
        "../src/*"
      ],
      "app/*": [
        "../src/app/*"
      ],
      "environments/*": [
        "../src/environments/*"
      ]
    },
    "outDir": "../out-tsc/spec",
    "module": "es2015",
    "types": [
      "jasmine",
      "node"
    ]
  },
  "files": [
    "test.ts",
    "test.tool.ts"
  ],
  "rules": {

  },
  "include": [
    "**/*.spec.ts",
    "**/*.d.ts"
  ]
}
