{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "*": [
        "*",
        "src/*",
        "app/*",
        "environments/*"
      ],
      "src/*": [
        "../src/*"
      ],
      "app/*": [
        "../src/app/*"
      ],
      "environments/*": [
        "../src/environments/*"
      ]
    },
    "outDir": "../out-tsc/app",
    "module": "es2015",
    "types": []
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ]
}
