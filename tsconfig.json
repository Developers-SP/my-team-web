{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "*": [
        "*",
        "src/*",
        "src/app/*",
        "src/environments/*"
      ],
      "src/*": [
        "src/*"
      ],
      "app/*": [
        "src/app/*"
      ],
      "environments/*": [
        "src/environments/*"
      ]
    },
    "moduleResolution": "node",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "jasmine",
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ]
  }
}
